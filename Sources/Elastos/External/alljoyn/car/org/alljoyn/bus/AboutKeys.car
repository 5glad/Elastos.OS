
module
{
    namespace Org {
    namespace Alljoyn {
    namespace Bus {

    interface IAboutKeys {
        /**
         * Required. The globally unique id for the application
         */
        const String ABOUT_APP_ID = "AppId";

        /**
         * Required. The default language supported by the device. IETF language
         * tags specified by RFC 5646.
         */
        const String ABOUT_DEFAULT_LANGUAGE = "DefaultLanguage";

        /**
         * Required. Set by platform specific means.
         */
        const String ABOUT_DEVICE_NAME = "DeviceName";

        /**
         * Required. Set using the platform specific means
         */
        const String ABOUT_DEVICE_ID = "DeviceId";

        /**
         * Required. This field specifies the Application Name. It is assigned by
         * the App Manufacturer
         */
        const String ABOUT_APP_NAME = "AppName";

        /**
         * Required. The manufacturer's name of the application.
         */
        const String ABOUT_MANUFACTURER = "Manufacturer";

        /**
         * Required. The application model number.
         */
        const String ABOUT_MODEL_NUMBER = "ModelNumber";

        /**
         * Required. This field returns the list of supported languages by the
         * device.
         */
        const String ABOUT_SUPPORTED_LANGUAGES = "SupportedLanguages";

        /**
         * Required. Detailed description
         */
        const String ABOUT_DESCRIPTION = "Description";

        /**
         * Optional. The date of manufacture using format YYYY-MM-DD (known as XML
         * DateTime Format)
         */
        const String ABOUT_DATE_OF_MANUFACTURE = "DateOfManufacture";

        /**
         * Required. The software version for the OEM software.
         */
        const String ABOUT_SOFTWARE_VERSION = "SoftwareVersion";

        /**
         * Required. The current version of the AllJoyn SDK utilized by the
         * application.
         */
        const String ABOUT_AJ_SOFTWARE_VERSION = "AJSoftwareVersion";

        /**
         * Optional. The device hardware version
         */
        const String ABOUT_HARDWARE_VERSION = "HardwareVersion";

        /**
         * Optional. The support URL to be populated by device OEM
         */
        const String ABOUT_SUPPORT_URL = "SupportUrl";
    }

    } // namespace Bus
    } // namespace Alljoyn
    } // namespace Org
}
