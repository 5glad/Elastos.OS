module
{
    namespace Elastos{
    namespace Apps {
    namespace Dailer {
    namespace CallLog {

    /**
     * @Involve
     * interface Elastos.Droid.View.IView
     * interface Elastos.Droid.View.IKeyEventCallback
     * interface Elastos.Droid.View.Accessibility.IAccessibilityEventSource
     * interface Elastos.Droid.Graphics.Drawable.IDrawableCallback
     */
    interface ICallTypeIconsView {
        Clear();

        Add(
            [in] Int32 callType);

        /**
         * Determines whether the video call icon will be shown.
         *
         * @param showVideo True where the video icon should be shown.
         */
        SetShowVideo(
            [in] Boolean showVideo);

        /**
         * Determines if the video icon should be shown.
         *
         * @return True if the video icon should be shown.
         */
        IsVideoShown(
            [out] Boolean* result);

        // @NeededForTesting
        GetCount(
            [out] Int32* count);

        // @NeededForTesting
        GetCallType(
            [in] Int32 index,
            [out] Int32* result);
    }

    } //CallLog
    } //Dailer
    } //Apps
    } //Elastos
}
