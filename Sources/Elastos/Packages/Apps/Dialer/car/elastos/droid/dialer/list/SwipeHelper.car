module
{
    using interface Elastos.Droid.View.IView;
    using interface Elastos.Droid.View.IMotionEvent;

    namespace Elastos {
    namespace Droid {
    namespace Dialer {
    namespace List {

    [local]
    interface ISwipeHelperCallback {

        GetChildAtPosition(
            [in] IMotionEvent* ev,
            [out] IView** child);

        GetChildContentView(
            [in] IView* v,
            [out] IView** child);

        OnScroll();

        CanChildBeDismissed(
            [in] IView* v,
            [out] Boolean* result);

        OnBeginDrag(
            [in] IView* v);

        OnChildDismissed(
            [in] IView* v);

        OnDragCancelled(
            [in] IView* v);

    }

    [local]
    interface ISwipeHelperOnItemGestureListener {
        OnSwipe(
            [in] IView* view);

        OnTouch();

        IsSwipeEnabled(
            [out] Boolean* result);
    }

    } //List
    } //Dialer
    } //Droid
    } //Elastos
}
