
module {

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Telephony {
    namespace Uicc {

    interface IAdnRecord {
        CreateFromParcel(
            [in] Parcel source,
            [out] AdnRecord* result);

        NewArray(
            [in] Int32 size);

        AdnRecord(
            [in] String alphaTag, 
            [in] String number);

        GetAlphaTag(
            [out] String* result);

        GetNumber(
            [out] String* result);

        GetEmails();

        GetAdditionalNumbers();

        IsEmpty(
            [out] Boolean* result);

        HasExtendedRecord(
            [out] Boolean* result);

        IsEqual(
            [in] AdnRecord adn,
            [out] Boolean* result);

        UpdateAnrEmailArray(
            [in] AdnRecord adn, 
            [in] Int32 emailFileNum, 
            [in] Int32 anrFileNum);

        DescribeContents(
            [out] Int32* result);

        WriteToParcel(
            [in] Parcel dest, 
            [in] Int32 flags);

        BuildAdnString(
            [in] Int32 recordSize);

    }

    }//Uicc
    }//Telephony
    }//Internal
    }//Droid
    }//Elastos

}
