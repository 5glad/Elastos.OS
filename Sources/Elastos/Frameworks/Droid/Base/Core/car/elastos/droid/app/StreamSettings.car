
module
{
    namespace Elastos {
    namespace Droid {
    namespace App {

    interface IStreamSettings {
        GetStreamId(
            [out] Int32* streamId);

        GetValue(
            [out] Int32* value);

        SetValue(
            [in] Int32 value);

        SetOverride(
            [in] Boolean override);

        IsOverride(
            [out] Boolean* override);

        IsDirty(
            [out] Boolean* dirty);

        GetXmlString(
            [in] IStringBuilder* builder,
            [in] IContext* context);
    }

    interface IStreamSettingsHelper {
        FromXml(
            [in] IXmlPullParser* xpp,
            [in] IContext* context,
            [out] IStreamSettings** obj);
    }

    } // namespace App
    } // namespace Droid
    } // namespace Elastos
}
