
module {

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Telephony {
    namespace DataConnection {

    interface IDcSwitchAsyncChannel {
        ReqConnect(
            [in] String type);

        RspConnect(
            [in] Message response,
            [out] Int32* result);

        ConnectSync(
            [in] String type,
            [out] Int32* result);

        ReqDisconnect(
            [in] String type);

        RspDisconnect(
            [in] Message response,
            [out] Int32* result);

        DisconnectSync(
            [in] String type,
            [out] Int32* result);

        ReqIsIdle();

        RspIsIdle(
            [in] Message response,
            [out] Boolean* result);

        IsIdleSync(
            [out] Boolean* result);

        ReqIsIdleOrDeacting();

        RspIsIdleOrDeacting(
            [in] Message response,
            [out] Boolean* result);

        IsIdleOrDeactingSync(
            [out] Boolean* result);

    }

    }//DataConnection
    }//Telephony
    }//Internal
    }//Droid
    }//Elastos

}
