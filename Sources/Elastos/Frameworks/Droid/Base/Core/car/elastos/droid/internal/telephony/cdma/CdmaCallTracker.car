
module {

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Telephony {
    namespace Cdma {

    interface ICdmaCallTracker {
        Dispose();

        RegisterForVoiceCallStarted(
            [in] Handler h, 
            [in] Int32 what, 
            [in] Object obj);

        UnregisterForVoiceCallStarted(
            [in] Handler h);

        RegisterForVoiceCallEnded(
            [in] Handler h, 
            [in] Int32 what, 
            [in] Object obj);

        UnregisterForVoiceCallEnded(
            [in] Handler h);

        RegisterForCallWaiting(
            [in] Handler h, 
            [in] Int32 what, 
            [in] Object obj);

        UnregisterForCallWaiting(
            [in] Handler h);


        IsInEmergencyCall(
            [out] Boolean* result);

    }

    }//Cdma
    }//Telephony
    }//Internal
    }//Droid
    }//Elastos

}
