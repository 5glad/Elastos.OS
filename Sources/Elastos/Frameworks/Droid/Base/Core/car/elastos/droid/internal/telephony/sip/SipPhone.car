
module {

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Telephony {
    namespace Sip {

    interface ISipPhone {
        SipPhone(
            [in] Context context, 
            [in] PhoneNotifier notifier, 
            [in] SipProfile profile);

        Equals(
            [in] Object o,
            [out] Boolean* result);

        GetSipUri(
            [out] String* result);

        Equals(
            [in] SipPhone phone,
            [out] Boolean* result);

        TakeIncomingCall(
            [in] Object incomingCall,
            [out] Connection* result);

        CanTransfer(
            [out] Boolean* result);

        ExplicitCallTransfer();

        ClearDisconnected();

        SendDtmf(
            [in] Char32 c);

        StartDtmf(
            [in] Char32 c);

        StopDtmf();

        SendBurstDtmf(
            [in] String dtmfString);

        GetOutgoingCallerIdDisplay(
            [in] Message onComplete);

        GetCallWaiting(
            [in] Message onComplete);

        SetCallWaiting(
            [in] Boolean enable, 
            [in] Message onComplete);

        SetEchoSuppressionEnabled();

        SetMute(
            [in] Boolean muted);

        GetMute(
            [out] Boolean* result);

        GetForegroundCall(
            [out] Call* result);

        GetBackgroundCall(
            [out] Call* result);

        GetRingingCall(
            [out] Call* result);

        GetServiceState(
            [out] ServiceState* result);

    }

    }//Sip
    }//Telephony
    }//Internal
    }//Droid
    }//Elastos

}
