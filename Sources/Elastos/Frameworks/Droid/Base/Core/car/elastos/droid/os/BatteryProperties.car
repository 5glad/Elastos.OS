module
{
    namespace Elastos {
    namespace Droid {
    namespace Os {

    /**
     * @Involve
     * interace IParcelable
     */
    interface IBatteryProperties {
        Set(
            [in] IBatteryProperties* other);

        SetChargerAcOnline(
            [in] Boolean value);

        SetChargerUsbOnline(
            [in] Boolean value);

        SetChargerWirelessOnline(
            [in] Boolean value);

        SetBatteryStatus(
            [in] Int32 value);

        SetBatteryHealth(
            [in] Int32 value);

        SetBatteryPresent(
            [in] Boolean value);

        SetBatteryLevel(
            [in] Int32 value);

        SetBatteryVoltage(
            [in] Int32 value);

        SetBatteryTemperature(
            [in] Int32 value);

        SetBatteryTechnology(
            [in] String value);

        GetChargerAcOnline(
            [out] Boolean* resutl);

        GetChargerUsbOnline(
            [out] Boolean* resutl);

        GetChargerWirelessOnline(
            [out] Boolean* resutl);

        GetBatteryStatus(
            [out] Int32* result);

        GetBatteryHealth(
            [out] Int32* result);

        GetBatteryPresent(
            [out] Boolean* resutl);

        GetBatteryLevel(
            [out] Int32* result);

        GetBatteryVoltage(
            [out] Int32* result);

        GetBatteryTemperature(
            [out] Int32* result);

        GetBatteryTechnology(
            [out] String* result);
    }

    } // namespace Os
    } // namespace Droid
    } // namespace Elastos
}
