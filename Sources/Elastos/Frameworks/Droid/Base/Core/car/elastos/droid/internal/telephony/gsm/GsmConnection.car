
module {

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Telephony {
    namespace Gsm {

    interface IGsmConnection {
        GsmConnection(
            [in] Context context, 
            [in] DriverCall dc, 
            [in] GsmCallTracker ct, 
            [in] Int32 index);

        GsmConnection(
            [in] Context context, 
            [in] String dialString, 
            [in] GsmCallTracker ct, 
            [in] GsmCall parent);

        Dispose();

        GetCall(
            [out] GsmCall* result);

        GetDisconnectTime(
            [out] Int64* result);

        GetHoldDurationMillis(
            [out] Int64* result);

        GetDisconnectCause(
            [out] Int32* result);

        GetState(
            [out] State* result);

        GetPostDialState(
            [out] PostDialState* result);

        ProceedAfterWaitChar();

        ProceedAfterWildChar(
            [in] String str);

        CancelPostDial();

        DisconnectCauseFromCode(
            [in] Int32 causeCode,
            [out] Int32* result);

        Update(
            [in] DriverCall dc);

    }

    }//Gsm
    }//Telephony
    }//Internal
    }//Droid
    }//Elastos

}
