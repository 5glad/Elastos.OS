
module {

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Telephony {

    interface IConnection {
        OnLocalVideoCapabilityChanged(
            [in] Boolean capable);

        GetCnapName(
            [out] String* result);

        GetOrigDialString(
            [out] String* result);

        GetCnapNamePresentation(
            [out] Int32* result);

        GetCreateTime(
            [out] Int64* result);

        GetConnectTime(
            [out] Int64* result);

        GetConnectTimeReal(
            [out] Int64* result);

        GetDurationMillis(
            [out] Int64* result);

        GetHoldingStartTime(
            [out] Int64* result);

        IsIncoming(
            [out] Boolean* result);

        GetState(
            [out] State* result);

        GetStateBeforeHandover(
            [out] State* result);

        GetExtras(
            [out] Bundle* result);

