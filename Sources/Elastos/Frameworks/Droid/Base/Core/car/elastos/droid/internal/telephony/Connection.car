            [out] Int32* result);

        GetCallSubstate(
            [out] Int32* result);

        SetVideoState(
            [in] Int32 videoState);

        SetLocalVideoCapable(
            [in] Boolean capable);

        SetRemoteVideoCapable(
            [in] Boolean capable);

        SetAudioQuality(
            [in] Int32 audioQuality);

        SetCallSubstate(
            [in] Int32 callSubstate);

