
module {

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Telephony {
    namespace DataConnection {

    interface IDcTracker {
        Dispose();

        GetAnyDataEnabled(
            [out] Boolean* result);

        GetAnyDataEnabled(
            [in] Boolean checkUserDataEnabled,
            [out] Boolean* result);

        IsDisconnected(
            [out] Boolean* result);

        HandleMessage(
            [in] Message msg);

        Update();

        CleanUpAllConnections(
            [in] String cause);

        UpdateRecords();

        CleanUpAllConnections(
            [in] String cause, 
            [in] Message disconnectAllCompleteMsg);

        RegisterForAllDataDisconnected(
            [in] Handler h, 
            [in] Int32 what, 
            [in] Object obj);

        UnregisterForAllDataDisconnected(
            [in] Handler h);

        SetInternalDataEnabledFlag(
            [in] Boolean enable,
            [out] Boolean* result);

        SetInternalDataEnabled(
            [in] Boolean enable,
            [out] Boolean* result);

        SetInternalDataEnabled(
            [in] Boolean enable, 
            [in] Message onCompleteMsg,
            [out] Boolean* result);

        SetDataAllowed(
            [in] Boolean enable, 
            [in] Message response);

        GetPcscfAddress(
            [in] String apnType);

        SetImsRegistrationState(
            [in] Boolean registered);

    }

    }//DataConnection
    }//Telephony
    }//Internal
    }//Droid
    }//Elastos

}
