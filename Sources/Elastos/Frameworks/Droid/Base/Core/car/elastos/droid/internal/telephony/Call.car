
module {

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Telephony {

    interface ICall {
        IsAlive(
            [out] Boolean* result);

        IsRinging(
            [out] Boolean* result);

        IsDialing(
            [out] Boolean* result);

        HasConnection(
            [in] Connection c,
            [out] Boolean* result);

        HasConnections(
            [out] Boolean* result);

        GetState(
            [out] State* result);

        GetExtras(
            [out] Bundle* result);

        IsIdle(
            [out] Boolean* result);

        IsGeneric(
            [out] Boolean* result);

        SetGeneric(
            [in] Boolean generic);

        HangupIfAlive();

    }

    }//Telephony
    }//Internal
    }//Droid
    }//Elastos

}
