
module {

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Telephony {
    namespace DataConnection {

    interface IDdsScheduler {
        GetInstance(
            [out] DdsScheduler* result);

        Init();

        AddRequest(
            [in] NetworkRequest req);

        RemoveRequest(
            [in] NetworkRequest req);

        MarkAccepted(
            [in] NetworkRequest req);

        IsAlreadyAccepted(
            [in] NetworkRequest nr,
            [out] Boolean* result);

        GetFirstWaitingRequest(
            [out] NetworkRequest* result);

        AcceptWaitingRequest(
            [out] Boolean* result);

        NotifyRequestAccepted(
            [in] NetworkRequest nr);

        IsDdsSwitchRequired(
            [in] NetworkRequest n,
            [out] Boolean* result);

        GetCurrentDds(
            [out] Int64* result);

        UpdateCurrentDds(
            [in] NetworkRequest n);

        GetSubIdFromNetworkRequest(
            [in] NetworkRequest n,
            [out] Int64* result);

        TriggerSwitch(
            [in] NetworkRequest n);

        IsMultiDataSupported(
            [out] Boolean* result);

        IsAnyRequestWaiting(
            [out] Boolean* result);

    }

    }//DataConnection
    }//Telephony
    }//Internal
    }//Droid
    }//Elastos

}
