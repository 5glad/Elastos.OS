
module {

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Telephony {
    namespace Uicc {

    interface IIccRecords {
        Dispose();

        RecordsRequired();

        GetAdnCache(
            [out] AdnRecordCache* result);

        GetIccId(
            [out] String* result);

        RegisterForRecordsLoaded(
            [in] Handler h, 
            [in] Int32 what, 
            [in] Object obj);

        UnregisterForRecordsLoaded(
            [in] Handler h);

        RegisterForImsiReady(
            [in] Handler h, 
            [in] Int32 what, 
            [in] Object obj);

        UnregisterForImsiReady(
            [in] Handler h);

        RegisterForRecordsEvents(
            [in] Handler h, 
            [in] Int32 what, 
            [in] Object obj);

        UnregisterForRecordsEvents(
            [in] Handler h);

        RegisterForNewSms(
            [in] Handler h, 
            [in] Int32 what, 
            [in] Object obj);

        UnregisterForNewSms(
            [in] Handler h);

        GetIMSI(
            [out] String* result);

        SetImsi(
            [in] String imsi);

        GetMsisdnNumber(
            [out] String* result);

        GetGid1(
            [out] String* result);

        GetMsisdnAlphaTag(
            [out] String* result);

        GetVoiceMailNumber(
            [out] String* result);

        GetServiceProviderName(
            [out] String* result);

        GetVoiceMailAlphaTag(
            [out] String* result);

        GetRecordsLoaded(
            [out] Boolean* result);

        HandleMessage(
            [in] Message msg);

        IsCspPlmnEnabled(
            [out] Boolean* result);

        GetOperatorNumeric(
            [out] String* result);

        IsCallForwardStatusStored(
            [out] Boolean* result);

        GetVoiceCallForwardingFlag(
            [out] Boolean* result);

        SetVoiceCallForwardingFlag(
            [in] Int32 line, 
            [in] Boolean enable, 
            [in] String number);

        IsProvisioned ();

        GetIsimRecords(
            [out] IsimRecords* result);

        GetUsimServiceTable(
            [out] UsimServiceTable* result);

        GetIccSimChallengeResponse(
            [in] Int32 authContext, 
            [in] String data,
            [out] String* result);

        GetSmsCapacityOnIcc(
            [out] Int32* result);

    }

    }//Uicc
    }//Telephony
    }//Internal
    }//Droid
    }//Elastos

}
