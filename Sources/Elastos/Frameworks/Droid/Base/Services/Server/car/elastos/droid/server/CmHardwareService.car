module
{
    namespace Elastos {
    namespace Droid {
    namespace Server {

    interface ICmHardwareInterface {

        GetSupportedFeatures(
            [out] Int32* result);

        Get(
            [in] Int32 feature,
            [out] Boolean* result);

        Set(
            [in] Int32 feature,
            [in] Boolean enable,
            [out] Boolean* result);

        GetDisplayColorCalibration(
            [out] ArrayOf<Int32>** result);

        SetDisplayColorCalibration(
            [in] ArrayOf<Int32>* rgb,
            [out] Boolean* result);

        GetNumGammaControls(
            [out] Int32* result);

        GetDisplayGammaCalibration(
            [in] Int32 idx,
            [out] ArrayOf<Int32>** result);

        SetDisplayGammaCalibration(
            [in] Int32 idx,
            [in] ArrayOf<Int32>* rgb,
            [out] Boolean* result);

        GetVibratorIntensity(
            [out] ArrayOf<Int32>** result);

        SetVibratorIntensity(
            [in] Int32 intensity,
            [out] Boolean* result);

        GetLtoSource(
            [out] String* result);

        GetLtoDestination(
            [out] String* result);

        GetLtoDownloadInterval(
            [out] Int64* result);

        GetSerialNumber(
            [out] String* result);

        RequireAdaptiveBacklightForSunlightEnhancement(
            [out] Boolean* result);
    }

    }//namespace Server
    }//namespace Droid
    }//namespace Elastos
}