
module
{
    using interface Org.W3c.Dom.INode;
    using interface Org.W3c.Dom.IDOMConfiguration;

    namespace Org {
    namespace Apache {
    namespace Harmony {
    namespace Xml {
    namespace Dom {

    /**
     * A minimal implementation of DOMConfiguration. This implementation uses inner
     * parameter instances to centralize each parameter's behavior.
     */

    /**
      * @Involve
      * interface IDOMConfiguration
      */
    interface IDOMConfigurationImpl {
        Normalize(
            [in] INode* node);
    }

    interface IParameter {
        Get(
            [in] IDOMConfigurationImpl* config,
            [out] IObject** result);

        Set(
            [in] IDOMConfigurationImpl* config,
            [in] IObject* value);

        CanSet(
            [in] IDOMConfigurationImpl* config,
            [in] IObject* value,
            [out] Boolean* result);
    }

    } // namespace Dom
    } // namespace Xml
    } // namespace Harmony
    } // namespace Apache
    } // namespace Org
}
